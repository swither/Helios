        <Label Grid.Column="0" Grid.Row="14" FontSize="10" HorizontalAlignment="Right">
            <Label.ToolTip>Theater name(s) for the base map, use comma separated list for multiple names</Label.ToolTip> Taiwan
        </Label>
        <HeliosSdk:HeliosTextBox  Grid.Column="1" Grid.Row="14" FontSize="10" Text="{Binding Control.BaseMapName_114, Mode=TwoWay}" Margin="2,2,0,2"></HeliosSdk:HeliosTextBox>
    </Grid>
</HeliosSdk:HeliosPropertyEditor>
