<HeliosSdk:HeliosPropertyEditor x:Class="GadrocsWorkshop.Helios.Controls.MapControlsBaseMapEditor" 
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:HeliosSdk="clr-namespace:GadrocsWorkshop.Helios.Windows.Controls;assembly=Helios"
             DataContext="{Binding RelativeSource={RelativeSource Self}}"
             Height="370" Width="275">
    <Grid Background="Transparent" Margin="0,0,0,0" HorizontalAlignment="Left" Width="275">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" MinWidth="79" />
            <ColumnDefinition Width="175" />
            <ColumnDefinition Width="10" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <TextBlock Grid.Column="0" Grid.ColumnSpan="3" Grid.Row="0" Margin="4,6,3,6" FontSize="12">Base Map Theater Names</TextBlock>

        <Label Grid.Column="0" Grid.Row="1" FontSize="10" HorizontalAlignment="Right">
            <Label.ToolTip>Theater name(s) for the base map, use comma separated list for multiple names</Label.ToolTip> Aegean
        </Label>
        <HeliosSdk:HeliosTextBox  Grid.Column="1" Grid.Row="1" FontSize="10" Text="{Binding Control.BaseMapName_101, Mode=TwoWay}" Margin="2,2,0,2"></HeliosSdk:HeliosTextBox>

        <Label Grid.Column="0" Grid.Row="2" FontSize="10" HorizontalAlignment="Right">
            <Label.ToolTip>Theater name(s) for the base map, use comma separated list for multiple names</Label.ToolTip> Balkans
        </Label>
        <HeliosSdk:HeliosTextBox  Grid.Column="1" Grid.Row="2" FontSize="10" Text="{Binding Control.BaseMapName_102, Mode=TwoWay}" Margin="2,2,0,2"></HeliosSdk:HeliosTextBox>

        <Label Grid.Column="0" Grid.Row="3" FontSize="10" HorizontalAlignment="Right">
            <Label.ToolTip>Theater name(s) for the base map, use comma separated list for multiple names</Label.ToolTip> Central Europe
        </Label>
        <HeliosSdk:HeliosTextBox  Grid.Column="1" Grid.Row="3" FontSize="10" Text="{Binding Control.BaseMapName_103, Mode=TwoWay}" Margin="2,2,0,2"></HeliosSdk:HeliosTextBox>

        <Label Grid.Column="0" Grid.Row="4" FontSize="10" HorizontalAlignment="Right">
            <Label.ToolTip>Theater name(s) for the base map, use comma separated list for multiple names</Label.ToolTip> EMF
        </Label>
